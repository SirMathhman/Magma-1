val math = import "FloatMath";

val Point : Point = <Point>[x : Float, y : Float, distance : (Point) => Float];

val SimplePoint = (x : Float, y : Float) => Point(x, y, (other : Point) => {
  val xDelta = x - other.x;
  val yDelta = y - other.y;
  val xSquared = math.pow(xDelta, 2.0f);
  val ySquared = math.pow(yDelta, 2.0f);
  val sum = xSquared + ySquared;
  return math.sqrt(sum);
});
